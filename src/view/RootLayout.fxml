<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.RootLayoutController">
   <top>
      <Pane prefHeight="40.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="HomeButton" mnemonicParsing="false" onAction="#handleHome" prefHeight="40.0" prefWidth="80.0" text="Home" visible="false">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button fx:id="LogoutButton" layoutX="720.0" mnemonicParsing="false" onAction="#handleLogout" prefHeight="40.0" prefWidth="80.0" text="Logout" visible="false">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </children>
      </Pane>
   </top>
</BorderPane>
